{
    "name": "singlequote/laravel-datatables",
    "description": "Datatables that supports pagination and recursive searching in relations",
    "keywords": [
        "Datatables",
        "Laravel",
		"Pagination",
		"Models"
    ],
    "homepage": "https://github.com/singlequote/Laravel-datatables",
    "license": "MIT",
    "version": "3.0.0",
    "authors": [
        {
            "name": "Wim Pruiksma",
            "email": "wim@acfbentveld.nl",
            "homepage": "https://www.quotec.nl/",
            "role": "CEO"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        
    },
    "autoload": {
        "psr-4": {
            "singlequote\\DataTables\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "singlequote\\DataTables\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "singlequote\\DataTables\\DataTablesServiceProvider"
            ],
            "aliases": {
                "DataTable": "singlequote\\DataTables\\DataTablesFacade"
            }
        }
    }
}
